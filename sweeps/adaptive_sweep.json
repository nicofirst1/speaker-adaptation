early_terminate:
  eta: 3
  min_iter: 10
  type: hyperband
method: bayes
metric:
  goal: maximize
  name: out_domain_val/modified_accs
  target: 1.0
project: speaker-influence-sweep

parameters:
    learning_rate:
        distribution: uniform
        max: 0.1
        min: 0.0001

    s_iter:
        distribution: int_uniform
        max: 8
        min: 1

    top_k:
        distribution: uniform
        max: 1.0
        min: 0.3









program: ../src/evals/adaptive_speaker.py

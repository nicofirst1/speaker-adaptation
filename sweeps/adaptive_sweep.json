early_terminate:
  eta: 3
  min_iter: 10
  type: hyperband
method: bayes
metric:
  goal: maximize
  name: out_domain_test/int_accs
  target: 1.0
project: speaker-influence-sweep

parameters:
    adapt_lr:
        distribution: uniform
        max: 0.1
        min: 0.001

    s_iter:
        distribution: int_uniform
        max: 8
        min: 1

    model_type:
        value : no_hist

    adaptive_loss:
        value: ce

    test_split:
        value : seen





program: ../src/evals/adaptive_speaker.py

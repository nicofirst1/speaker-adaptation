early_terminate:
  eta: 4
  min_iter: 10
  type: hyperband
method: bayes
metric:
  goal: maximize
  name: eval/int_list_accuracy
  target: 1.0
project: inter-sweep

parameters:
    learning_rate:
        distribution: q_uniform
        max: 0.01
        min: 0.0001
        q:   0.0005


    data_domain:
        value: all

    model_type:
        value: tom

    attention_dim:
      values:
        - 128
        - 512

    embedding_dim:
      values:
        - 128
        - 512

    hidden_dim:
      values:
        - 128
        - 512


    dropout_prob:
      distribution: q_uniform
        max: 0.5
        min: 0.0
        q:   0.01










program: ../src/trainers/interpreter_pretrain.py

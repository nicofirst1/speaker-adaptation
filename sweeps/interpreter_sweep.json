early_terminate:
  eta: 4
  min_iter: 10
  type: hyperband
method: bayes
metric:
  goal: maximize
  name: eval/int_list_accuracy
  target: 1.0
project: inter-sweep

parameters:
    learning_rate:
        distribution: uniform
        max: 0.6
        min: 0.001

    data_domain:
        value: all

    model_type:
        value: tom

    attention_dim:
      values:
        - 128
        - 512
        - 1024

    embedding_dim:
      values:
        - 128
        - 512
        - 1024

    hidden_dim:
      values:
        - 128
        - 512
        - 1024


    dropout_prob:
      distribution: uniform
      max: 0.1
      min: 0.0









program: ../src/trainers/interpreter_pretrain.py

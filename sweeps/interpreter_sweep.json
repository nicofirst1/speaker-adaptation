early_terminate:
  eta: 3
  min_iter: 10
  type: hyperband
method: bayes
metric:
  goal: maximize
  name: eval/int_list_accuracy
  target: 1.0
project: inter-sweep

parameters:
    learning_rate:
        distribution: uniform
        max: 1
        min: 0.0001

    data_domain:
        value: all

    model_type:
        value: dev

    attention_dim:
      values:
        - 128
        - 512
        - 1024
        - 2042

    embedding_dim:
      values:
        - 128
        - 512
        - 1024
        - 2042

    hidden_dim:
      values:
        - 128
        - 512
        - 1024

    num_layers_emb2hid:
      values:
        - 1
        - 2
        - 3
        - 4

    num_layers_contx:
      values:
        - 1
        - 2
        - 3
        - 4
    num_layers_mm:
      values:
        - 1
        - 2
        - 3
        - 4
    num_layers_sep:
      values:
        - 1
        - 2
        - 3
        - 4


    dropout_prob:
      distribution: uniform
      max: 0.9
      min: 0.0









program: ../src/trainers/interpreter_pretrain.py

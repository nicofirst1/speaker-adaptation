early_terminate:
  eta: 3
  min_iter: 10
  type: hyperband
method: bayes
metric:
  goal: maximize
  name: eval/list_acc
  target: 1.0
project: ec-sweep

parameters:
    learning_rate:
        distribution: q_uniform
        max: 1e-6
        min: 1e-7
        q:   2e-7


    train_domain:
        value: all

    entropy_loss_weight:
      distribution: q_uniform
      max: 0.5
      min: 0.0
      q: 0.1

    policy_loss_weight:
      distribution: q_uniform
      max: 1.0
      min: 0.5
      q: 0.1


    sampler_temp:
      distribution: q_uniform
      max: 0.6
      min: 0.1
      q: 0.1










program: ../src/trainers/ec_pretrain.py

early_terminate:
  eta: 3
  min_iter: 80
  type: hyperband
method: bayes
metric:
  goal: maximize
  name: diff_eval/list_acc
  target: 1.0
project: ec-sweep

parameters:
    learning_rate:
        distribution: q_uniform
        max: 1e-3
        min: 1e-5
        q:   1e-5


    train_domain:
        value: all

    policy_loss_weight:
      distribution: q_uniform
      max: 1.0
      min: 0.5
      q: 0.1


    dropout_prob:
      distribution: q_uniform
      max: 0.9
      min: 0.0
      q: 0.1










program: ../src/finetuning/speak_ec_finetune.py

early_terminate:
  eta: 3
  min_iter: 10
  type: hyperband
method: bayes
metric:
  goal: maximize
  name: eval/sweep_metric
  target: 10.0
project: speaker

parameters:
    learning_rate:
        distribution: uniform
        max: 1
        min: 0.0001

    embedding_dim:
      values:
        - 512
        - 1024
        - 2042

    hidden_dim:
      values:
        - 512
        - 1024
        - 128

    dropout_prob:
      distribution: uniform
      max: 0.9
      min: 0.0









program: ../src/trainers/speaker_train.py

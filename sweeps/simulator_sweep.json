early_terminate:
  eta: 3
  min_iter: 5
  type: hyperband
method: bayes
metric:
  goal: maximize
  name: eval/sim_list_accuracy
  target: 1.0
project: sim-sweep

parameters:
    learning_rate:
        distribution: q_uniform
        max: 0.001
        min: 0.0001
        q:   0.0001


    data_domain:
        value: all


    attention_dim:
      values:
        - 512
        - 1024

    embedding_dim:
      values:
        - 512
        - 1024

    hidden_dim:
      values:
        - 512
        - 1024


    dropout_prob:
      distribution: q_uniform
      max: 0.5
      min: 0.0
      q: 0.01










program: ../src/trainers/simulator_pretrain.py
